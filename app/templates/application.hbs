<div class="container-fluid container-wrap">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 flash-messages">
      {{#each flashMessages.queue as |flash|}}
        {{#flash-message flash=flash as |component flash|}}
          <p>{{flash.message}}</p>
        {{/flash-message}}
      {{/each}}
    </div>
  </div>
  {{outlet}}
</div>

